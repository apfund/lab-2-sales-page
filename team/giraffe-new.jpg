		const theme = localStorage.getItem('upswing-body-theme') || 'upswing-theme-light';
		document.body.classList.add(...theme.split(" "));

		function setHeaderData(username, cartSize, has_lab) {
			const header = document.getElementById("header-background");

			if(username !== null) {
				document.querySelectorAll("[full-username]").forEach(e => e.innerHTML = username);
				if(header) header.classList.add("upswing-is-logged-in");
			} else {
				document.querySelectorAll("[full-username]").forEach(e => e.setAttribute("full-username", ''));
				if(header?.classList?.contains("upswing-is-logged-in")) {
					header.classList.remove("upswing-is-logged-in");
				}
			}
			
			const cartContent = Number(cartSize);
			document.querySelectorAll("[cart-content-count]").forEach(e => e.setAttribute("cart-content-count", (isNaN(cartContent) || cartContent < 1) ? 0 : cartContent));

			document.querySelectorAll("[has-lab]").forEach(e => e.setAttribute("has-lab", has_lab ? 1 : 0));
		}

		function update_user_info_by_ajax() {
			fetch("https://upswingpoker.com/wp-content/plugins/upswing-custom-header/ajax-user-info.php", {
				method: 'GET',
				credentials: 'include',
				headers: {
					"dataType": "json"
				}
			})
			.then(response => response.json())
			.then(({ username, basket_size, has_lab }) => {
				if(document.readyState == "loading") {
					window.addEventListener('DOMContentLoaded', (event) => {
						setHeaderData(username, basket_size, has_lab);
					});
				} else {
					setHeaderData(username, basket_size, has_lab);
				}
			})
			.catch(err => {
				console.log(err);
			});
		}

		update_user_info_by_ajax();
	</script>

